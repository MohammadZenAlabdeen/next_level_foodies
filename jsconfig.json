{  
    "compilerOptions": {  
      "target": "es6",  
      "module": "commonjs",  
      "baseUrl": ".",  
      "paths": {  
        "@/*": ["./*"]  
      },  
      "jsx": "preserve",  
      "moduleResolution": "node",  
      "allowSyntheticDefaultImports": true,  
      "strict": true,  
      "esModuleInterop": true,  
      "skipLibCheck": true  
    },  
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],  
    "exclude": ["node_modules"]  
  }  